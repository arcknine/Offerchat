.header.group
  %a#root-path-link.logo
  %ul.header-inline-list.primary-user-nav
    %li
      %a.header-nav-reports
        Reports
    %li
      %a.header-nav-history#history-menu-link
        History
    %li.profile-status
      %a#profile-status-toggle
