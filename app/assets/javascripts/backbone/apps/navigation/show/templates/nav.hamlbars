          %a
            %span.status.signout
              %i.icon.icon-small-x
            Signout
        .dropdown-pointer
    %li.profile-settings.header-settings
      %a#settings-dropdown-toggle
        %i.icon.icon-gear-large
      %ul.settings-dropdown.hide.dropdowns
        %li
          %a#settings-menu-link
            Settings
        %li
          %a#websites-menu-link
            Websites
        %li
          %a#agent-menu-link
            Agent Management
        %li
          %a#account-menu-link
            Account
        .dropdown-pointer